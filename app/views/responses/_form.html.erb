<%= form_for(@response, :html => {:multipart => true})  do |f| %>
  <div class="form-group">
    <div>
      <%= f.text_field :exterior_color, autofocus: true, placeholder: "Make", class: "form-control", required: true %>
      <%= f.hidden_field :proposal_id, value: params[:id] %>
    </div>
  </div>
  <%= f.submit "Post Request" %>
<% end %>